{
    "name": "larangular/email-record",
    "description": "Keep a record of emails",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php"                          : ">=7.1",
        "illuminate/support"           : "~5.8.0|^6.0|^7.0|^8.0",
        "illuminate/mail"              : "~5.8.0|^6.0|^7.0|^8.0",
        "larangular/routing-controller": "^0.1.0|^0.2.0|^0.3.0",
        "larangular/support"           : "^0.1.0|^0.2.0",
        "larangular/installable"       : "^0.1.0|^0.2.0",
        "larangular/migration-package" : "^0.1.0|^0.2.0"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\EmailRecord\\": "./src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\EmailRecord\\EmailRecordServiceProvider"
            ],
            "aliases"  : {
                "EmailReport": "Larangular\\EmailRecord\\Facades\\EmailReport"
            }
        }
    }
}
