{
    "name": "larangular/email-record",
    "description": "Keep a record of emails",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.*",
        "illuminate/mail": "5.*",
        "larangular/routing-controller": "0.1.*",
        "larangular/support": "0.1.*",
        "larangular/installable": "0.1.*",
        "larangular/migration-package": "0.1.*"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\EmailRecord\\": "./src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\EmailRecord\\EmailRecordServiceProvider"
            ],
            "aliases"  : {
                "EmailReport": "Larangular\\EmailRecord\\Facades\\EmailReport"
            }
        }
    }
}
